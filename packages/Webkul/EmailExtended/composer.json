{
    "name": "webkul/email-extended",
    "description": "Extended email management system with threads, tracking, and scheduling for Krayin CRM",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Webkul",
            "email": "support@webkul.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "webkul/email": "*",
        "webkul/email-template": "*"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\EmailExtended\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\EmailExtended\\Providers\\ModuleServiceProvider",
                "Webkul\\EmailExtended\\Providers\\EmailExtendedServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}