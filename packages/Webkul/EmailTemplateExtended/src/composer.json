{
    "name": "webkul/email-template-extended",
    "description": "Extended Email Template module for Krayin CRM with advanced features",
    "type": "library",
    "keywords": [
        "krayin",
        "crm",
        "email-template",
        "email",
        "template",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "CoreSys Team",
            "email": "dev@coresys.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "krayin/laravel-crm": "^1.3|^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\EmailTemplateExtended\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webkul\\EmailTemplateExtended\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\EmailTemplateExtended\\Providers\\EmailTemplateServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}